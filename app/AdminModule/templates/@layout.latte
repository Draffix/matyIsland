<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3.org/TR/html4/loose.dtd">
<html>
    <head>
        <title>MatyLand - Administrace</title>
        <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
        <link rel="stylesheet" type="text/css" href="{$basePath}/css/administration/style.css">

        <!-- NiftyGrid -->
        <link rel="stylesheet" type="text/css" href="{$basePath}/css/administration/grid.css">
        <script type="text/javascript" src="{$basePath}/js/niftygrid/grid.js"></script>
        <script type="text/javascript" src="{$basePath}/js/niftygrid/jquery-ui.js"></script>
        
        <!-- bootstrap -->
        <link href="{$basePath}/css/administration/bootstrap/bootstrap.css" rel="stylesheet">
        <link href="{$basePath}/css/administration/bootstrap/bootstrap-responsive.css" rel="stylesheet">

        <link rel="stylesheet" type="text/css" href="{$basePath}/css/piro.css"/>

        <script type="text/javascript" src="{$basePath}/js/jquery.min.js"></script>
        <script type="text/javascript" src="{$basePath}/js/administration/ddaccordion.js"></script>
        <script src="{$basePath}/js/administration/jquery.jclock-1.2.0.js.txt" type="text/javascript"></script>
        <script type="text/javascript" src="{$basePath}/js/administration/jconfirmaction.jquery.js"></script>
        <script type="text/javascript" src="{$basePath}/js/administration/assets.js"></script>
        <script type="text/javascript" src="{$basePath}/js/administration/jquery.ba-hashchange.js"></script>       

        <!-- javascript TinyMCE -->
        <script type="text/javascript" src="{$basePath}/js/tiny_mce/tiny_mce.js"></script>

<!-- javascript Pirobox -->
        <script type="text/javascript" src="{$basePath}/js/jquery.min.js"></script>
        <script type="text/javascript" src="{$basePath}/js/jquery-ui-1.8.2.custom.min.js"></script>
        <script type="text/javascript" src="{$basePath}/js/pirobox_extended.js"></script>

        <script type="text/javascript" >
        tinyMCE.init({
            language : "cs",
            mode: "specific_textareas",
            editor_selector: "mceEditor",
            theme : "advanced",
            height : "300",
            width : "535",
        });
        </script >
        
        <script type="text/javascript">
	
                $(document).ready(function() {
                        $('.ask').jConfirmAction();
                });
	
        </script>
        <script type="text/javascript">
        $(function($) {
            $('.jclock').jclock();
        });
        </script>
    </head>

    <body>
        <div id="main_container">
            {include header.latte}
            <div class="main_content">
                {include header-content.latte}
                <div class="center_content">  
<!--                     {include navigation.latte}-->
                    {include #content}
                </div>
                <div class="clear"></div>
            </div>       
            {include footer.latte}
        </div>
    </body>
</html>